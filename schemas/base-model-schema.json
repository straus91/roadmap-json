		"model": {
			"description": "Model description",
			"$comment": "ROADMAP:00002",
			"type": "object",
			"properties": {
				"Name": {
					"description": "Model name",
					"type": "string"
				},
				"Indexing code": {
					"$ref": "#/$defs/indexing"
				},
				"Author": {
					"description": "Author(s) (ROADMAP:00064)",
					"type": "array",
					"items": {
						"$ref": "#/$defs/person"
					}
				},
				"Organization": {
					"description": "Developing or sponsoring Organization(s) (ROADMAP:00072)",
					"type": "array",
					"items": {
						"$ref": "#/$defs/org"
					}
				},
				"Date": {
					"type": "object",
					"properties": {
						"Created": {
							"description": "Creation date (ROADMAP:03419)",
							"$ref": "#/$defs/date"
