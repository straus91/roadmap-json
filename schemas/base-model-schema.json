							"Stereoscopy (RID50131)",
							"Stereotactic radiography (RID50260)",
							"Stimulated acoustic emission (RID10916)",
							"Strain imaging (RID10933)",
							"Tomography (RID28840)",
							"Tomosynthesis (RID10359)",
							"Triplex ultrasound (RID10923)",
							"US-RF (RID49581)",
							"Ultrasound (RID10326)",
							"Volume imaging (RID10930)",
							"XR-RF (RID49582)"
						]
					}
				},
				"Procedure": {
					"description": "Imaging procedures, from LOINC/RSNA Radiology Playbook",
					"$comment": "Imaging procedure (ROADMAP:00121)",
					"type": "array",
					"items": {
						"type": "object",
						"minProperties": 1,
						"maxProperties": 3,
						"properties": {
							"LOINC": {
								"type": "string",
								"examples": [
									"36813-4"
								]
							},
							"RadLex": {
								"type": "string",
								"minLength": 5,
								"maxLength": 10,
								"pattern": "RPID[0-9]+",
								"examples": [
									"RPID145"
								]
							},
							"Name": {
								"type": "string",
								"examples": [
									"CT Abdomen and Pelvis W contrast IV"
								]
							}
						},
						"additionalProperties": false,
						"examples": [
							{
								"Name": "XR Hand - left PA",
								"LOINC": "36623-7"
							},
							{
								"Name": "CT Abdomen and Pelvis W contrast IV",
								"LOINC": "36813-4",
								"RadLex": "RPID145"
							}
						]
					}
				},
				"File format": {
					"description": "Image file format",
					"type": "array",
					"items": {
						"type": "string",
						"enum": [
							"DICOM",
							"JPEG",
							"NiFTI",
							"PNG",
							"Other"
						]
					},
					"unevaluatedItems": true
				},
				"Resolution": {
					"type": "string"
				},
				"Pre-processing": {
					"type": "string"
				},
				"Burned-in PHI": {
					"type": "string",
					"enum": [
						"Yes",
						"No",
						"Unknown"
					]
				}
			}
		},
		"indexing": {
			"description": "Indexing code(s): 2-letter content codes (e.g., 'NR'), RadLex, and/or SNOMED CT",
			"type": "object",
			"properties": {
				"Content": {
					"$ref": "#/$defs/content-code"
				},
				"RadLex": {
					"$ref": "#/$defs/radlex"
				},
				"SNOMED": {
					"description": "SNOMED CT code(s)",
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				}
			},
			"additionalProperties": false
		},
		"license": {
			"description": "License or rights to access this resource",
			"$comment": "ROADMAP:00068",
			"minProperties": 1,
			"properties": {
				"Text": {
					"type": "string",
					"examples": [
						"Non-commercial use"
					]
				},
				"URL": {
					"type": "string",
					"format": "uri",
					"examples": [
						"https://creativecommons.org/licenses/by-nc-nd/4.0/"
					]
				}
			},
			"additionalProperties": false
		},
